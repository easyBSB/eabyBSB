{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/easybsb-client/src",
  "prefix": "easy-bsb",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/easybsb-client",
        "index": "apps/easybsb-client/src/index.html",
        "main": "apps/easybsb-client/src/main.ts",
        "polyfills": "apps/easybsb-client/src/polyfills.ts",
        "tsConfig": "apps/easybsb-client/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/easybsb-client/src/favicon.ico",
          "apps/easybsb-client/src/assets"
        ],
        "styles": ["apps/easybsb-client/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/easybsb-client/src/environments/environment.ts",
              "with": "apps/easybsb-client/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "easybsb-client:build:development",
        "proxyConfig": "apps/easybsb-client/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "browserTarget": "easybsb-client:build:production"
        },
        "development": {
          "browserTarget": "easybsb-client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "easybsb-client:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/easybsb-client/src/**/*.ts",
          "apps/easybsb-client/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/easybsb-client"],
      "options": {
        "jestConfig": "apps/easybsb-client/jest.config.js",
        "passWithNoTests": true
      }
    },
    "e2e": {
      "executor": "./libs/executors:playwright",
      "outputs": ["coverage/apps/easybsb-client"]
    }
  },
  "tags": []
}
