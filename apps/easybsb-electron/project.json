{
  "sourceRoot": "apps/easybsb-electron/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx-electron:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/easybsb-electron",
        "main": "apps/easybsb-electron/src/main.ts",
        "tsConfig": "apps/easybsb-electron/tsconfig.app.json",
        "generatePackageJson": true,
        "implicitDependencies": [
          "dist/package/easybsb.tar.gz"
        ],
        "assets": [
          "apps/easybsb-electron/src/assets"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/easybsb-electron/src/environments/environment.ts",
              "with": "apps/easybsb-electron/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "nx-electron:execute",
      "options": {
        "buildTarget": "easybsb-electron:build"
      }
    },
    "package": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm run build easybsb-electron -- --configuration=production",
          "node ./bin/build-electron.js"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/easybsb-electron/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/easybsb-electron"
      ],
      "options": {
        "jestConfig": "apps/easybsb-electron/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
