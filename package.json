{
  "author": {
    "name": "easybsb"
  },
  "description": "easybsb app",
  "name": "easy-bsb",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "start:all": "nx run-many --target=serve --all --parallel",
    "start:all-e2e": "nx run-many --target=serve --configuration=e2e --all --parallel",
    "build:all": "nx run-many --target=build --configuration=production --all --parallel",
    "build:all:no-cache": "npm run build:all -- --skip-nx-cache",
    "build:executors": "npm run build --prefix ./libs/executors",
    "package:server": "nx run server:package",
    "package:electron": "nx run electron:package",
    "postinstall": "npm run build:executors && ngcc --properties es2015 browser module main",
    "e2e": "nx run web:e2e"
  },
  "private": true,
