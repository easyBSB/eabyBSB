  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "start:all": "nx run-many --target=serve --all --parallel",
    "start:all-e2e": "nx run-many --target=serve --configuration=e2e --all --parallel",
    "build:all": "nx run-many --target=build --configuration=production --all --parallel",
    "build:all:no-cache": "npm run build:all -- --skip-nx-cache",
    "build:executors": "npm run build --prefix ./libs/executors",
    "build:easybsb-parser": "npm run build easybsb-parser",
    "package:easybsb-parser": "tar -cvf bin/easybsb-parser.tar.gz -C dist/libs/easybsb-parser .",
    "package:server": "nx run server:package",
    "package:electron": "nx run electron:package",
    "postinstall": "npm run build:executors && ngcc --properties es2020 browser module main",
    "install:easybsb-parser": "npm run build easybsb-parser && npm run package:easybsb-parser && npm i ./bin/easybsb-parser.tar.gz",
    "e2e": "nx run web:e2e",
    "e2e:dev": "nx run web:e2e-dev",
    "i18n:sync": "nx run web:i18n-sync"
  },
  "private": true,
